{
  "name": "gutenku",
  "version": "2.0.0",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "lint": "pnpm -r run lint",
    "lint:css": "pnpm --filter @gutenku/front lint:css",
    "typecheck": "pnpm -r --if-present --filter '!./private/**' run typecheck",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "pnpm -r --if-present --filter '!./private/**' run test",
    "start": "pnpm --filter @gutenku/server dev",
    "dev": "pnpm -r --if-present --filter '!./private/**' --parallel run dev",
    "setup": "pnpm --filter @gutenku/server run setup:books",
    "setup:reset": "pnpm --filter @gutenku/server run setup:books:reset",
    "extract": "pnpm --filter @gutenku/server extract",
    "train": "pnpm --filter @gutenku/server train",
    "post": "pnpm --filter @gutenku/server post",
    "build": "pnpm -r --if-present --filter '!./private/**' run build",
    "prepare": "husky",
    "submodule:update": "./scripts/update-submodule.sh"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^20.3.1"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0"
  },
  "packageManager": "pnpm@10.27.0"
}
