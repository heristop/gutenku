{
  "name": "gutenku",
  "version": "1.5.0",
  "private": true,
  "licence": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "lint": "pnpm --filter @gutenku/api lint && pnpm --filter @gutenku/vue lint",
    "test": "pnpm --filter @gutenku/api test && pnpm --filter @gutenku/vue test",
    "start": "pnpm --filter @gutenku/api dev",
    "next": "pnpm --filter @gutenku/vue dev",
    "dev": "npm-run-all --parallel start next",
    "setup": "pnpm --filter @gutenku/api setup",
    "extract": "pnpm --filter @gutenku/api extract",
    "train": "pnpm --filter @gutenku/api train",
    "post": "pnpm --filter @gutenku/api post",
    "build": "pnpm --filter @gutenku/vue build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "lint-staged": {
    "*.{cjs,js,ts,vue,md}": [
      "pnpm lint",
      "pnpm test"
    ]
  }
}
