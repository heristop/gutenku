{
  "name": "gutenku",
  "version": "2.0.0",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "pnpm -r run test",
    "start": "pnpm --filter @gutenku/server dev",
    "dev": "pnpm -r --parallel run dev",
    "setup": "pnpm --filter @gutenku/server run setup:books",
    "setup:reset": "pnpm --filter @gutenku/server run setup:books:reset",
    "extract": "pnpm --filter @gutenku/server extract",
    "train": "pnpm --filter @gutenku/server train",
    "post": "pnpm --filter @gutenku/server post",
    "build": "pnpm --filter @gutenku/front build",
    "prepare": "husky"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^20.3.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0"
  },
  "packageManager": "pnpm@10.16.1"
}
