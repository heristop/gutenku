{
  "name": "gutenku",
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "lint": "npm-run-all --parallel lint:server lint:front",
    "lint:server": "pnpm --filter @gutenku/server lint",
    "lint:front": "pnpm --filter @gutenku/front lint",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "npm-run-all --parallel test:server test:front",
    "test:server": "pnpm --filter @gutenku/server test",
    "test:front": "pnpm --filter @gutenku/front test",
    "start": "pnpm --filter @gutenku/server dev",
    "next": "pnpm --filter @gutenku/front dev",
    "dev": "npm-run-all --parallel start next",
    "setup": "pnpm --filter @gutenku/server run setup:books",
    "setup:reset": "pnpm --filter @gutenku/server run setup:books:reset",
    "extract": "pnpm --filter @gutenku/server extract",
    "train": "pnpm --filter @gutenku/server train",
    "post": "pnpm --filter @gutenku/server post",
    "build": "pnpm --filter @gutenku/front build",
    "prepare": "husky"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^20.2.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0"
  },
  "packageManager": "pnpm@10.16.1",
  "licence": "UNLICENSED"
}
