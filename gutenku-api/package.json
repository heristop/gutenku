{
    "name": "gentuku-api",
    "version": "1.0.0",
    "private": true,
    "main": "src/index.ts",
    "scripts": {
        "start": "node --loader ts-node/esm --max-old-space-size=54000 src/index.ts",
        "dev": "nodemon --watch './**/*.ts' --exec 'node --max-old-space-size=54000 --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
        "lint": "eslint . --fix --ignore-path .gitignore",
        "fetch": "python3 bin/fetch_books.py",
        "post": "ts-node-esm bin/post_haiku.ts",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.ts ./__tests__"
    },
    "type": "module",
    "dependencies": {
        "apollo-server-core": "^3.11.1",
        "apollo-server-express": "^3.11.1",
        "canvas": "^2.11.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "global": "^4.4.0",
        "graphql": "^16.6.0",
        "instagram-publisher": "^2.1.1",
        "mongoose": "^6.9.0",
        "openai": "^3.1.0",
        "request": "^2.88.2",
        "syllable": "^5.0.1",
        "terminal-image": "^2.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.11.19",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^13.1.1",
        "nodemon": "^2.0.20",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^4.9.5"
    },
    "lint-staged": {
        "*": "yarn lint"
    }
}