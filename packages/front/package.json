{
  "name": "@gutenku/front",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "start": "vite serve --host",
    "preview": "vite preview",
    "test": "cypress run",
    "test:a11y": "cypress run --spec 'cypress/e2e/accessibility.cy.ts'",
    "test:gui": "cypress run --headed --no-exit",
    "lint": "oxlint --fix",
    "lint:css": "stylelint 'src/**/*.{vue,scss,css}'",
    "typecheck": "vue-tsc --noEmit",
    "lighthouse": "vite build && lhci autorun",
    "cap:build": "pnpm build && npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "cap:sync": "npx cap sync",
    "generate:assets": "npx tsx scripts/generate-capacitor-assets.ts",
    "cap:assets": "pnpm generate:assets && npx capacitor-assets generate"
  },
  "dependencies": {
    "@capacitor-community/safe-area": "^8.0.1",
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/share": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@fingerprintjs/botd": "^2.0.0",
    "@gutenku/shared": "workspace:*",
    "@unhead/vue": "^2.1.2",
    "@urql/vue": "^2.0.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/motion": "^3.0.3",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "lucide-vue-next": "^0.562.0",
    "marked": "^17.0.1",
    "mermaid": "^11.12.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.6",
    "@capacitor/assets": "^3.0.5",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@lhci/cli": "^0.15.1",
    "@types/node": "^25.0.9",
    "@types/webfontloader": "^1.6.38",
    "@types/zen-observable": "^0.8.7",
    "@vitejs/plugin-vue": "^6.0.3",
    "axe-core": "^4.11.1",
    "beasties": "^0.3.5",
    "cypress": "^15.9.0",
    "cypress-axe": "^1.7.0",
    "postcss-html": "^1.8.1",
    "purgecss": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.97.2",
    "sass-loader": "^16.0.6",
    "sharp": "^0.34.5",
    "stylelint": "^17.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vite-ssg": "^28.2.2",
    "vue-tsc": "^3.2.2"
  },
  "licence": "UNLICENSED"
}
