{
  "name": "@gutenku/api",
  "version": "1.8.0",
  "private": true,
  "licence": "UNLICENSED",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --loader ts-node/esm --use-strict src/index.ts",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm --use-strict' src/index.ts",
    "lint": "eslint . --fix",
    "setup": "python3 scripts/setup.py",
    "extract": "node --loader ts-node/esm scripts/extract_haiku.ts",
    "train": "node --loader ts-node/esm scripts/markov_train.ts",
    "post": "node --loader ts-node/esm scripts/post_haiku.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@graphql-tools/schema": "^10.0.3",
    "@typescript-eslint/typescript-estree": "^8.3.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "canvas": "^3.2.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "form-data": "^4.0.0",
    "global": "^4.4.0",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "loglevel": "^1.9.1",
    "mongoose": "^8.8.2",
    "natural": "^8.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.38.5",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
    "sharp": "^0.33.5",
    "syllable": "^5.0.1",
    "terminal-image": "^2.0.0",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/typescript-estree": "^8.3.0",
    "@vitest/coverage-istanbul": "^2.1.4",
    "canvas": "^2.11.2",
    "eslint": "^9.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.1.4"
