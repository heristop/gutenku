{
  "name": "@gutenku/server",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "pnpm --filter @gutenguess/server run build || true && tsc -p tsconfig.json && tsc-alias",
    "start": "tsx src/index.ts",
    "start:dist": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "oxlint --fix",
    "typecheck": "tsc --noEmit",
    "setup:books": "tsx src/cli/setup-books.ts",
    "setup:books:reset": "tsx src/cli/setup-books.ts --reset",
    "setup:books:py": "python3 scripts/setup.py",
    "extract": "tsx scripts/extract-haiku.ts",
    "ga:evolve": "tsx scripts/ga-evolution.ts",
    "ga:benchmark": "tsx scripts/ga-benchmark.ts",
    "train": "NODE_OPTIONS='--expose-gc' tsx scripts/markov-train.ts",
    "fetch:classics": "tsx scripts/fetch-gutenberg-classics.ts",
    "post": "tsx scripts/post-haiku.ts",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/schema": "^10.0.31",
    "@gutenku/shared": "workspace:*",
    "axios": "^1.13.2",
    "body-parser": "^2.2.2",
    "canvas": "^3.2.1",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.5",
    "global": "^4.4.0",
    "graphql": "^16.12.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.6",
    "helmet": "^8.1.0",
    "log-update": "^7.0.2",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^9.1.3",
    "natural": "^8.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.16.0",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "seedrandom": "^3.0.5",
    "sharp": "^0.34.5",
    "string-width": "^8.1.0",
    "syllable": "^5.0.1",
    "terminal-image": "^4.2.0",
    "tsyringe": "^4.10.0",
    "ws": "^8.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.8",
    "@types/node-fetch": "^2.6.13",
    "@types/seedrandom": "^3.0.8",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-istanbul": "^4.0.17",
    "@vitest/coverage-v8": "4.0.17",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "optionalDependencies": {
    "@gutenguess/server": "file:../../private/gutenguess/packages/server"
  },
  "licence": "UNLICENSED"
}
