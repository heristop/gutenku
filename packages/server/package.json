{
    "name": "gutenku-api",
    "version": "1.3.0",
    "private": true,
    "licence": "UNLICENSED",
    "main": "src/index.ts",
    "scripts": {
        "start": "node --loader ts-node/esm src/index.ts",
        "dev": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm' src/index.ts",
        "lint": "eslint . --fix --ignore-path .gitignore",
        "setup": "python3 scripts/setup.py",
        "extract": "ts-node-esm scripts/extract_haiku.ts",
        "train": "ts-node-esm scripts/markov_train.ts",
        "post": "ts-node-esm scripts/post_haiku.ts",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.ts ./__tests__"
    },
    "type": "module",
    "dependencies": {
        "@apollo/server": "^4.5.0",
        "canvas": "^2.11.2",
        "cli-progress": "^3.12.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "global": "^4.4.0",
        "graphql": "^16.6.0",
        "instagram-publisher": "^2.1.1",
        "mongoose": "^7.3.3",
        "natural": "^6.3.0",
        "openai": "^3.1.0",
        "request": "^2.88.0",
        "sharp": "^0.32.1",
        "syllable": "^5.0.1",
        "terminal-image": "^2.0.0"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^4.1.7",
        "@types/cli-progress": "^3.11.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.4.0",
        "@types/natural": "^5.1.2",
        "@types/node": "^18.15.11",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.36.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.0",
        "nodemon": "^2.0.21",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^5.0.3"
    },
    "lint-staged": {
        "*": "yarn lint"
    }
}
