{
    "name": "@gutenku/api",
    "version": "1.4.0",
    "private": true,
    "licence": "UNLICENSED",
    "main": "src/index.ts",
    "scripts": {
        "start": "node --loader ts-node/esm --use-strict src/index.ts",
        "dev": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm --use-strict' src/index.ts",
        "lint": "eslint . --fix --ignore-path .gitignore",
        "setup": "python3 scripts/setup.py",
        "extract": "ts-node-esm scripts/extract_haiku.ts",
        "train": "ts-node-esm scripts/markov_train.ts",
        "post": "ts-node-esm scripts/post_haiku.ts",
        "test": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.ts ./__tests__"
    },
    "type": "module",
    "dependencies": {
        "@apollo/server": "^4.9.4",
        "canvas": "^2.11.2",
        "cli-progress": "^3.12.0",
        "commander": "^11.0.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "global": "^4.4.0",
        "graphql": "^16.8.1",
        "graphql-subscriptions": "^2.0.0",
        "graphql-ws": "^5.14.2",
        "instagram-publisher": "^2.1.1",
        "mongoose": "^7.6.0",
        "natural": "^6.8.0",
        "openai": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
        "sharp": "^0.32.6",
        "syllable": "^5.0.1",
        "terminal-image": "^2.0.0",
        "tsyringe": "^4.8.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^4.1.7",
        "@types/cli-progress": "^3.11.0",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.6",
        "@types/natural": "^5.1.3",
        "@types/node": "^18.15.11",
        "@types/node-fetch": "^2.6.2",
        "@types/ws": "^8.5.8",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.36.0",
        "eslint-config-standard-with-typescript": "^39.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.0.1",
        "nodemon": "^3.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.0",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "*": "yarn lint"
    }
}
